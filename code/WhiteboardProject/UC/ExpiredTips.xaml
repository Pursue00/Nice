<Window x:Class="WhiteboardProject.UC.ExpiredTips"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WhiteboardProject.UC"
        mc:Ignorable="d"
        Title="ExpiredTips" Height="250" Width="500" WindowStartupLocation="CenterScreen" WindowStyle="None" ResizeMode="NoResize">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Label x:Name="tbTime" FontSize="20"/>
            <TextBox x:Name="tbBox" FontSize="20" BorderBrush="{x:Null}" BorderThickness="0"/>
        </StackPanel>
        <Button x:Name="btnOK" Width="100" Height="40" Content="确定" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0,0,50,20" Click="btnOK_Click"/>
    </Grid>
</Window>
